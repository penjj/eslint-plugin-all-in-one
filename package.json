{
  "name": "eslint-plugin-all-in-one",
  "version": "0.2.0",
  "author": "peng.j.1995@gmail.com",
  "description": "eslint config all in one.",
  "main": "lib/index.js",
  "packageManager": "pnpm@8.6.0",
  "files": [
    "lib",
    "prettier"
  ],
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext=js,vue,ts,tsx --fix",
    "release": "bumpp -r"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-css-import-order": "^1.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unocss": "0.0.1-alpha.3",
    "eslint-plugin-vue": "^9.17.0",
    "prettier": "^2.8.8",
    "vue-eslint-parser": "^9.3.1"
  },
  "devDependencies": {
    "bumpp": "^9.2.0",
    "eslint-plugin-all-in-one": "workspace:*",
    "typescript": "~5.0.4"
  },
  "peerDependencies": {
    "eslint": "^8.42.0"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "plugin:all-in-one/import",
      "plugin:all-in-one/prettier",
      "plugin:all-in-one/react",
      "plugin:all-in-one/solid",
      "plugin:all-in-one/typescript",
      "plugin:all-in-one/unocss",
      "plugin:all-in-one/vue"
    ],
    "ignorePatterns": []
  },
  "prettier": "eslint-plugin-all-in-one/prettier"
}
